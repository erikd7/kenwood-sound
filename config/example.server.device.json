{
  "role": "server",
  "device_name": "Kitchen-Pi",
  "snapserver": {
    "name": "HomeAudio",
    "ports": {
      "audio": 1704,
      "control": 1705
    },
    "codec": "flac",
    "sample_format": "48000:16:2",
    "buffer_ms": 100,
    "streams": {
      "librespot": true,
      "airplay": true,
      "plexamp": true,
      "host_audio": true
    }
  },
  "snapweb": {
    "enabled": true,
    "ports": {
      "http": 1780
    }
  },
  "plexamp": {
    "stream_name": "Plexamp"
  },
  "host_audio": {
    "stream_name": "External Audio Input",
    "device": "find from aplay -l",
    "sample_format": "48000:16:2"
  }
}
