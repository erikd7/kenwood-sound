{
  "role": "both",
  "device_name": "Kitchen",
  "snapserver": {
    "name": "MyHome",
    "ports": {
      "audio": 1704,
      "control": 1705
    },
    "codec": "flac",
    "sample_format": "48000:16:2",
    "buffer_ms": 100,
    "streams": {
      "librespot": false,
      "airplay": false,
      "plexamp": false,
      "host_audio": false
    }
  },
  "snapweb": {
    "enabled": false,
    "ports": {
      "http": 1780
    }
  },
  "librespot": {
    "stream_name": "Spotify",
    "device_type": "speaker",
    "bitrate": 320,
    "initial_volume": 80,
    "volume_steps": 100
  },
  "airplay": {
    "stream_name": "AirPlay"
  },
  "plexamp": {
    "stream_name": "Plexamp"
  },
  "snapclient": {
    "server_host": "localhost",
    "server_port": 1704,
    "output_device": "snapclient_dac"
  }
}
